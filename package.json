{
          "name": "atropa-mustache-comb",
          "version": "2022.11.16",
          "description": "A utility wrapper around mustache.",
          "main": "./atropa-mustache-comb.js",
          "devDependencies": {
                    "atropa-jsformatter": "git+https://github.com/matthewkastor/atropa-jsformatter.git#gh-pages",
                    "atropa-jslint": "git+https://github.com/matthewkastor/atropa-jslint.git#master",
                    "jsdoc-toolkit": "git+https://github.com/matthewkastor/node-jsdoc-toolkit.git#fix-tests"
          },
          "scripts": {
                    "buildDocs": "jsdoc-toolkit -v -d=./docs/jsdoc/ -r -s node_modules/atropa-header/src/ ./atropa-mustache-comb.js",
                    "lint": "atropa-jslint ./atropa-mustache-comb.js",
                    "srcFormat": "atropa-jsformatter ./atropa-mustache-comb.js ./atropa-mustache-comb.js",
                    "buildVsdocs": "jsdoc-toolkit -v -d=./docs/vsdoc/ -t=./node_modules/jsdoc-toolkit/templates/vsdoc/ -r -s node_modules/atropa-header/src/ ./atropa-mustache-comb.js"
          },
          "homepage": "https://github.com/matthewkastor/atropa-mustache-comb/",
          "bugs": "https://github.com/matthewkastor/atropa-mustache-comb/issues",
          "repository": {
                    "type": "git",
                    "url": "git://github.com/matthewkastor/atropa-mustache-comb.git"
          },
          "keywords": [
                    "mustache",
                    "template",
                    "utilities",
                    "atropa"
          ],
          "author": {
                    "name": "Matthew Kastor",
                    "email": "matthewkastor@gmail.com",
                    "url": "https://plus.google.com/100898583798552211130"
          },
          "licenses": [
                    {
                              "type": "GPL-3.0",
                              "url": "http://www.gnu.org/licenses/gpl-3.0-standalone.html"
                    }
          ],
          "dependencies": {
                    "atropa-string": "git+https://github.com/matthewkastor/atropa-string.git#master",
                    "mustache": "git+https://github.com/janl/mustache.js.git#master"
          },
          "engines": {
                    "node": ">=0.8.14"
          }
}
